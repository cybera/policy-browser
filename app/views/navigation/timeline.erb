<div class="panel panel-default">
  <% grouped_submissions.each do | date_arrived, submissions_on_date | %>
    <div class="panel-heading">
      <h5 class="panel-title"><%= int_to_datestr(date_arrived) %></h5>
    </div>
    <ul class="list-group query-matches">
      <% submissions_on_date.each do | submission | %>
        <li class="list-group-item">
          <a href='<%= "/browser?ppn=#{ppn}&submission=#{submission[:id]}&navigation=timeline" %>'>
            <%= "#{submission[:case]}-#{submission[:name]}" %>
          </a>
        </li>
      <% end %>
    </ul>
  <% end %>
</div>