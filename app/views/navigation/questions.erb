<ul class="list-group">
  <% questions.each do | q | %>
    <li class="list-group-item">
      <b><%= q.ref %></b>: <%= q['content'] %><br/><br/>
      <a href='<%= "/browser?ppn=#{ppn}&question=#{q['id']}&navigation=questions&detail=matches" %>'>
        Related Segments
      </a><br/>
    </li>
  <% end %>
</ul>